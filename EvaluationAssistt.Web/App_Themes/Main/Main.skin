    <Paddings PaddingLeft="8px" />
    <ButtonStyle Width="20px">
    </ButtonStyle>
 </dx:ASPxTimeEdit>

<dx:ASPxListBox runat="server" CssClass="simple_form" Border-BorderColor="#cccccc"></dx:ASPxListBox>

 <asp:ImageButton CssClass="imgButton" runat="server" />
 
