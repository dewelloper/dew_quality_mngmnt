 </dx:ASPxSpinEdit>

 <dx:ASPxDateEdit runat="server" AllowNull="false" Height="28px" Font-Names="Arial" Border-BorderColor="#cccccc" CssClass="simple_form">
    <Buttons>
        <dx:DropDownButton Visible="false"></dx:DropDownButton>
    </Buttons>
</dx:ASPxDateEdit>

